{
    "extends": "airbnb-base",
    "rules": {
        // "linebreak-style": ["error", "windows"],
        "linebreak-style": 0,
        
        "eslint-disable": 0, 
        "eslint-disable-line": 0, 
        "eslint-disable-next-line": 0,
        
        "no-underscore-dangle": ["error", {"allow": ["_id"]}],
        "no-console": "off",
        "func-names": ["error", "never"],
        "eqeqeq": "off",
        "no-useless-escape": "off",
        "no-unused-vars": "off"
    }
}